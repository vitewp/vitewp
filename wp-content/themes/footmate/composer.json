{
  "name": "tentypdev/footmate",
  "description": "FootMATE",
  "version": "0.1.4",
  "license": "GPLv3",
  "autoload": {
    "psr-4": {
      "FM\\": "app/"
    }
  },
  "require": {
    "php": ">=8.2",
    "symfony/var-dumper": "^7.2",
    "rosell-dk/webp-convert": "^2.9",
    "illuminate/view": "^12.10",
    "illuminate/support": "^12.10",
    "illuminate/translation": "^12.10",
    "illuminate/validation": "^12.10"
  },
  "scripts": {
    "lint": "phpcs",
    "format": "phpcbf",
    "analyze": "phpstan analyse --memory-limit=2048M"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.12",
    "wp-coding-standards/wpcs": "^3.1",
    "phpstan/phpstan": "^2.1",
    "szepeviktor/phpstan-wordpress": "^2.0",
    "php-stubs/wp-cli-stubs": "^2.11",
    "php-stubs/acf-pro-stubs": "^6.3",
    "php-stubs/woocommerce-stubs": "^9.8"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
